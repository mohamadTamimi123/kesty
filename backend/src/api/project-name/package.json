{
  "name": "project-name",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@nestjs/common",
        "@nestjs/core"
      ],
      "allowedVersions": {
        "@nestjs/common": "11",
        "@nestjs/core": "11"
      }
    }
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed:admin": "ts-node -r tsconfig-paths/register src/database/seeds/run-seed.ts",
    "seed:cities-categories": "ts-node -r tsconfig-paths/register src/database/seeds/run-seed.ts",
    "ensure:db": "ts-node -r tsconfig-paths/register scripts/ensure-database.ts",
    "create:tables": "ts-node -r tsconfig-paths/register scripts/create-tables.ts",
    "alter:categories": "ts-node -r tsconfig-paths/register scripts/alter-categories-table.ts",
    "cleanup:conversations": "ts-node -r tsconfig-paths/register scripts/cleanup-conversations.ts",
    "add:review-token-fields": "ts-node -r tsconfig-paths/register scripts/add-review-request-token-fields.ts",
    "add:review-customer-fields": "ts-node -r tsconfig-paths/register scripts/add-review-customer-fields.ts",
    "create:educational-articles-table": "ts-node -r tsconfig-paths/register scripts/create-educational-articles-table.ts",
    "seed:educational-articles": "ts-node -r tsconfig-paths/register src/database/seeds/run-educational-articles-seed.ts",
    "seed:projects": "ts-node -r tsconfig-paths/register src/database/seeds/run-projects-seed.ts",
    "load-test:projects": "ts-node -r tsconfig-paths/register scripts/load-test-projects.ts",
    "analyze:logs": "ts-node -r tsconfig-paths/register scripts/analyze-logs.ts",
    "load-test:full": "ts-node -r tsconfig-paths/register scripts/load-test-with-analysis.ts",
    "setup:seed": "ts-node -r tsconfig-paths/register scripts/setup-and-seed.ts"
  },
  "dependencies": {
    "@nestjs/bull": "^11.0.4",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.10",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^11.1.10",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.1",
    "google-auth-library": "^9.0.0",
    "ioredis": "^5.3.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.3",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
